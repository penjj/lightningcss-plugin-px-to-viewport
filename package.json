{
  "name": "lightningcss-plugin-px-to-viewport",
  "version": "0.0.1",
  "type": "module",
  "packageManager": "pnpm@8.6.0",
  "description": "A lightningcss plugin that converts px to viewport width",
  "keywords": [
    "lightningcss",
    "plugin",
    "px",
    "viewport",
    "width"
  ],
  "homepage": "https://github.com/unnok/lightningcss-plugin-px-to-viewport",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "release": "bumpp -r && pnpm publish"
  },
  "files": ["dist"],
  "dependencies": {
    "lightningcss": "^1.21.5"
  },
  "devDependencies": {
    "tsup": "^7.1.0",
    "vitest": "^0.33.0",
    "eslint-plugin-all-in-one": "^0.1.1",
    "bumpp": "^9.1.1"
  }
}
